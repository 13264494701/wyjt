<%@ page contentType="text/html;charset=UTF-8" %>
<%@ include file="/WEB-INF/views/include/taglib.jsp"%>
<html>
<head>
	<title>优放充值记录管理</title>
	<meta name="decorator" content="default"/>
	<script type="text/javascript">

	</script>
</head>
<body>
	<ul class="nav nav-tabs">
		<li><a href="${ufang}/recharge/">优放充值记录列表</a></li>
		<li class="active"><a href="${ufang}/recharge/query?id=${ufangRchgRecord.id}">优放充值记录查看</a></li>
	</ul><br/>
	<form:form id="inputForm" modelAttribute="ufangRchgRecord"  method="post" class="form-horizontal">
